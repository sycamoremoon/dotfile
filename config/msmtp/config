defaults
auth on
tls  on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account gmail
host smtp.gmail.com
port 587
tls_starttls on
from sycamoremoon376@gmail.com
user sycamoremoon376@gmail.com
passwordeval "pass sycamoremoon376@gmail.com"
logfile ~/.config/msmtp/msmtp.log

account disroot
host disroot.org
port 465
tls_starttls off
from zzixian@disroot.org
user zzixian@disroot.org
passwordeval "pass zzixian@disroot.org"
logfile ~/.config/msmtp/msmtp.log

account sdu
host smtp.vip.163.com
port 465
tls_starttls off
from 202200120267.sdu@vip.163.com
user 202200120267.sdu@vip.163.com
passwordeval "pass 202200120267.sdu@vip.163.com"
logfile ~/.config/msmtp/msmtp.log

account default: disroot
